<body>
    <div class="container">
        <!-- Page Content goes here -->


        <div class="card">
            <div class="card-image waves-effect waves-block waves-light">
                <img class="activator" src="images/bbq.jpg">
            </div>
            <div class="card-content">
                <span class="card-title activator grey-text text-darken-4">The grill neads your meat!!!
                    <i class="material-icons right">more_vert</i>
                </span>
                <p>
                    <a href="/new">I WANT TO BRING MY MEAT</a>
                </p>
            </div>
            <div class="card-reveal">
                <span class="card-title grey-text text-darken-4">Card Title
                    <i class="material-icons right">close</i>
                </span>
                <p>Here is some more information about this product that is only revealed once clicked on.</p>
            </div>
        </div>

    </div>

    </div>

    <table class="centered highlight white">
        <thead>
            <tr>
                <th>Name:</th>
                <th>Gender:</th>
                <th>Meat:</th>
            </tr>
        </thead>

        <tbody>
            {{#each attendee}}
            <tr>
                <td>
                    <a href="/users/{{_id}}">{{name}}</a>
                </td>
                <td>
                    {{gender}}
                </td>
                <td>
                    {{meat}}
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    </div>
